{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "run",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/build/index.js",
			"stopOnEntry": false,
			"cwd": "${workspaceRoot}",
			"runtimeExecutable": null,
			"runtimeArgs": [
				"--nolazy"
			],
			"env": {
				"NODE_ENV": "development"
			},
			"console": "internalConsole",
			"protocol": "inspector",
			"preLaunchTask": "build",
			"sourceMaps": true,
			"outFiles": [ "${workspaceRoot}/build/**/*.js" ]
		},
		{
			"name": "self-test",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/build/index.js",
			"stopOnEntry": false,
			"args": ["self-test"],
			"cwd": "${workspaceRoot}",
			"runtimeExecutable": null,
			"runtimeArgs": [
				"--nolazy"
			],
			"env": {
				"NODE_ENV": "development"
			},
			"console": "internalConsole",
			"protocol": "inspector",
			"preLaunchTask": "build",
			"sourceMaps": true,
			"outFiles": [ "${workspaceRoot}/build/**/*.js" ]
		}
	]
}
